{
  "compilerOptions": {
    "target": "ES2020",
    "strict": true,
    "preserveConstEnums": true,
    "noEmit": true,
    "sourceMap": false,
    "module": "ES2015",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@openapi-generated": ["./openapi/generated.ts"],
      "@common": ["./common/*"]
    }
  },
  "exclude": ["node_modules"]
}
